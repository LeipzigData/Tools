   ical:url <http://www.dresdner59.de/frauenseminar/> ;
   ical:creator <http://leipziger-ecken.de/Data/Person/P174> ;
   ical:dtstart "2016-06-04T11:00:01" ;
   ical:dtend "1000-01-01T18:00:01" ;
